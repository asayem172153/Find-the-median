        "data = [7, 2, 5, 1, 9, 3]\n",
        "\n",
        "data = sorted(data)\n",
        "length = len(data)\n",
        "\n",
        "if length % 2 == 0:\n",
        "  median = (data[length//2] + data[length//2-1] ) / 2 # avg of mid, mid-1 data\n",
        "else:\n",
        "  median = data[length//2]\n",
        "\n",
        "print('Median',median)"
